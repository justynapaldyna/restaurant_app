<div id="<%= dom_id product %>">
  <p>
    <strong>Name:</strong>
    <%= product.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= product.description %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= product.price %>
  </p>

  <p>
    <strong>Vegan:</strong>
    <%= product.vegan %>
  </p>

  <p>
    <strong>Vegetarian:</strong>
    <%= product.vegetarian %>
  </p>

  <p>
    <strong>Available:</strong>
    <%= product.available %>
  </p>

  <p>
    <strong>Catering:</strong>
    <%= product.catering %>
  </p>

  <p>
    <strong>Featured:</strong>
    <%= product.featured %>
  </p>

</div>
